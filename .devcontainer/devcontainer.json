{
  "name": "PromptBattle Devcontainer",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/app",
  "postCreateCommand": "npm i",
  "remoteUser": "node",
  "customizations": {
    "vscode": {
      "extensions": [
        "mhutchie.git-graph",
        "eamodio.gitlens",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "streetsidesoftware.code-spell-checker",
        "ms-vsliveshare.vsliveshare"
      ]
    }
  }
}